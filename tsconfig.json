{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2015",
		"outDir": "dist",
		"noFallthroughCasesInSwitch": false,
		"noImplicitAny": false,
		"noImplicitReturns": false,
		"noImplicitThis": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"skipLibCheck": true,
		"sourceMap": true,
		"strictNullChecks": false
	},
	"include": [
		"src/**/*.ts"
	]
}